package br.com.mattec.consul.verify;

public class RetiraTracoDoCep {

	public static void main(String[] args) {
		
		
		String volta = retiraTraco("09320-720");
		System.out.println(volta);
		
	}

	public static String retiraTraco(String cepComTraco) {

		String[] split = cepComTraco.split("-", 0);
		String cepSemTraco = "";
		for (String cp : split) {
			cepSemTraco += cp;
		}

		return cepSemTraco;
	}
}