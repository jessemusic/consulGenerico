package br.com.mattec.consul.verify;

public class RetiraTracoDoCep {

	public static String retiraTraco(String cepComTraco) {

		if (cepComTraco.contains("-")) {

			String[] split = cepComTraco.split("-", 0);
			String cepSemTraco = "";
			for (String cp : split) {
				cepSemTraco += cp;
			}

			return cepSemTraco;
		} else {
			return cepComTraco;
		}

	}
}